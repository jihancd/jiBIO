<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kontak.KontakFragment">


    <LinearLayout
        android:layout_width="208dp"
        android:layout_height="221dp"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35">

        <LinearLayout
            android:layout_width="188dp"
            android:layout_height="44dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="146dp">

            <ImageView
                android:id="@+id/iv_kontak_phone"
                android:src="@drawable/phone"
                tools:layout_editor_absoluteX="47dp"
                tools:layout_editor_absoluteY="152dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/tv_kontak_phone"
                android:layout_marginTop="12dp"
                android:text="081395457825"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="12dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_biodata_nama"
                tools:layout_editor_absoluteX="0dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="188dp"
            android:layout_height="44dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="146dp">

            <ImageView
                android:id="@+id/iv_kontak_email"
                android:src="@drawable/email"
                tools:layout_editor_absoluteX="47dp"
                tools:layout_editor_absoluteY="152dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/tv_kontak_email"

                android:layout_marginTop="12dp"
                android:text="jhncandri@gmail.com"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="12dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_biodata_nama"
                tools:layout_editor_absoluteX="0dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="188dp"
            android:layout_height="44dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="146dp">

            <ImageView
                android:id="@+id/iv_kontak_ig"
                android:src="@drawable/instagram"
                tools:layout_editor_absoluteX="47dp"
                tools:layout_editor_absoluteY="152dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/tv_kontak_ig"
                android:layout_marginTop="12dp"
                android:text="jihandinasty"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="12dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_biodata_nama"
                tools:layout_editor_absoluteX="0dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="188dp"
            android:layout_height="44dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="146dp">

            <ImageView
                android:id="@+id/iv_kontak_f"
                android:src="@drawable/facebook"
                tools:layout_editor_absoluteX="47dp"
                tools:layout_editor_absoluteY="152dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/tv_kontak_f"
                android:layout_marginTop="12dp"
                android:text="Jihan Dinasty"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="12dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_biodata_nama"
                tools:layout_editor_absoluteX="0dp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"/>
        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>